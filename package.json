{
  "name": "cheerio_cms",
  "description": "uses cheerio to parse HTML and manipulate the DOM",
  "version": "1.0.1",
  "author": "Michael Kreil <github_contact@michael-kreil.de>",
  "license": "Unlicense",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "check": "npm run lint && npm run fmt && npm run test && npm run build",
    "lint": "eslint . --color",
    "fmt": "prettier --write src",
    "prepack": "npm run build",
    "release": "npm run check && npm publish",
    "test-coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "upgrade": "npm-check-updates -u && rm -f package-lock.json && rm -rf node_modules && npm i"
  },
  "dependencies": {
    "cheerio": "^1.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "^30.2.0",
    "npm-check-updates": "^19.4.0",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  }
}
