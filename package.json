{
  "name": "cheerio_cms",
  "description": "uses cheerio to parse HTML and manipulate the DOM",
  "version": "1.1.0",
  "author": "Michael Kreil <github_contact@michael-kreil.de>",
  "license": "Unlicense",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelKreil/cheerio_cms.git"
  },
  "bugs": {
    "url": "https://github.com/MichaelKreil/cheerio_cms/issues"
  },
  "homepage": "https://github.com/MichaelKreil/cheerio_cms#readme",
  "keywords": [
    "cheerio",
    "html",
    "dom",
    "cms"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json && npm run doc",
    "check": "npm run lint && npm run format:check && npm run test && npm run build",
    "doc": "npm run doc:graph && npm run doc:typescript",
    "doc:graph": "vrt deps-graph | vrt doc-insert README.md '## Dependency Graph'",
    "doc:typescript": "npx vrt doc-typescript -f html",
    "lint": "eslint . --color",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "prepack": "npm run build",
    "release": "vrt release-npm",
    "test-coverage": "vitest run --coverage",
    "test": "vitest run",
    "upgrade": "vrt deps-upgrade"
  },
  "dependencies": {
    "cheerio": "^1.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/node": "^25.2.3",
    "@versatiles/release-tool": "^2.7.2",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vitest": "^4.0.18"
  }
}
